ivityName=".activities.MainActivity"                android:theme="@style/AppTheme">            <meta-data                    android:name="android.support.PARENT_ACTIVITY"                    android:value="jahirfiquitiva.apps.medicode.activities.MainActivity"/>        </activity>        <activity                android:name=".activities.CreatePatientActivity"                android:label="@string/title_activity_create_patient"                android:parentActivityName=".activities.MainActivity"                android:theme="@style/AppTheme">            <meta-data                    android:name="android.support.PARENT_ACTIVITY"                    android:value="jahirfiquitiva.apps.medicode.activities.MainActivity"/>        </activity>        <activity                android:name=".activities.DoctorAppntmntsActivity"                android:parentActivityName=".activities.MainActivity"                android:theme="@style/AppTheme">            <meta-data                    android:name="android.support.PARENT_ACTIVITY"                    android:value="jahirfiquitiva.apps.medicode.activities.MainActivity"/>        </activity>        <activity                android:name=".activities.PatientAppntmntsActivity"                android:parentActivityName=".activities.MainActivity"                android:theme="@style/AppTheme">            <meta-data                    android:name="android.support.PARENT_ACTIVITY"                    android:value="jahirfiquitiva.apps.medicode.activities.MainActivity"/>        </activity>        <activity                android:name=".activities.CreateAppntmntActivity"                android:label="@string/appntmnts"                android:parentActivityName=".activities.MainActivity"                android:theme="@style/DatePicker">            <meta-data                    android:name="android.support.PARENT_ACTIVITY"                    android:value="jahirfiquitiva.apps.medicode.activities.MainActivity"/>        </activity>    </application></manifest>